{
  "name": "RNTestApp",
  "version": "1.0.0",
  "scripts": {
    "android:prebuild": "npx expo prebuild --platform android",
    "android:build": "cd android && ./gradlew clean && ./gradlew assembleRelease -Dorg.gradle.jvmargs=\"-Xmx4096m -XX:MaxMetaspaceSize=1024m\" && cd ./app/build/outputs/apk/release && open .",
    "android:bundle": "cd android && ./gradlew clean && ./gradlew bundleRelease && cd ./app/build/outputs/bundle/release && open .",
    "ios:prebuild": "npx expo prebuild --platform ios",
    "postinstall": "patch-package",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "nuke": "rm -rf node_modules; rm -rf ios/build; rm -rf ios/Pods; rm -rf ios/Podfile.lock; rm -rf Gemfile.lock; rm -rf vendor; gem cleanup; npm cache clean --force",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.3",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/bottom-tabs": "^7.0.0-rc.30",
    "@react-navigation/elements": "^2.0.0-rc.22",
    "@react-navigation/native": "^7.0.0-rc.18",
    "@react-navigation/stack": "^7.0.0-rc.24",
    "@shopify/flash-list": "1.7.1",
    "@zhenyudu/react-native-snap-carousel": "^3.9.4",
    "colyseus.js": "^0.14.13",
    "expo": "^52.0.25",
    "expo-application": "~6.0.1",
    "expo-build-properties": "~0.13.1",
    "expo-checkbox": "~4.0.0",
    "expo-device": "~7.0.1",
    "expo-image": "~2.0.3",
    "expo-navigation-bar": "~4.0.7",
    "expo-screen-orientation": "~8.0.2",
    "expo-splash-screen": "~0.29.20",
    "lodash": "^4.17.21",
    "lottie-react-native": "7.1.0",
    "react": "18.3.1",
    "react-native": "0.76.6",
    "react-native-animatable": "^1.4.0",
    "react-native-autoheight-webview": "^1.6.5",
    "react-native-background-timer": "2.4.1",
    "react-native-blob-util": "^0.19.11",
    "react-native-calculator": "^0.5.2",
    "react-native-calendars": "^1.1306.0",
    "react-native-collapsible-tab-view": "^8.0.0",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-minimize": "^1.0.12",
    "react-native-orientation-locker": "^1.7.0",
    "react-native-otp-verify": "^1.1.8",
    "react-native-pager-view": "6.5.1",
    "react-native-pdf": "6.6.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "~4.4.0",
    "react-native-screenshot-prevent": "^1.2.1",
    "react-native-share": "^11.0.2",
    "react-native-simple-toast": "^3.3.1",
    "react-native-svg": "15.8.0",
    "react-native-svg-charts": "^5.4.0",
    "react-native-swipe-gestures": "^1.0.5",
    "react-native-swiper-flatlist": "^3.2.5",
    "react-native-tab-view": "^3.5.2",
    "react-native-view-shot": "~4.0.3",
    "react-native-webview": "13.12.5",
    "react-native-youtube": "2.0.2",
    "react-redux": "^7.2.3",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "socket.io-client": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.5.5",
    "@types/react": "~18.3.12",
    "@types/react-native-background-timer": "^2.0.2",
    "@types/react-native-svg-charts": "^5.0.16",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "chalk": "^5.3.0",
    "eslint": "^8.57.0",
    "eslint-config-universe": "^12.0.1",
    "patch-package": "^8.0.0",
    "prettier": "^3.2.5",
    "typescript": "~5.3.3"
  },
  "eslintConfig": {
    "extends": "universe/native",
    "root": true
  },
  "main": "node_modules/expo/AppEntry.js",
  "expo": {
    "install": {
      "exclude": [
        "react-native-safe-area-context"
      ]
    }
  },
  "private": true
}
